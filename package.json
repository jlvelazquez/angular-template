{
  "name": "angular-template",
  "version": "1.0.0",
  "description": "An Angular 1.x template with ES2015 and Webpack.",
  "main": "index.js",
  "scripts": {
    "start": "npm run copy-files && webpack-dev-server --env.dev",
    "build": "npm run copy-files && webpack --env.dev",
    "build:prod": "npm run copy-files && webpack --env.prod -p -d",
    "copy-files": "copyfiles -u 1 src/images/** dist",
    "test": "nyc --reporter=lcov node test.js | tap-spec",
    "docs": "esdoc -c esdoc.json"
  },
  "keywords": [
    "webpack",
    "angular",
    "es2015"
  ],
  "author": "Gustavo Mu√±oz <timbergus@gmail.com>",
  "license": "MIT",
  "private": true,
  "dependencies": {
    "angular": "1.5.3",
    "angular-new-router": "0.5.3",
    "angular-ui-router": "0.3.2",
    "moment": "2.12.0",
    "ng-redux": "3.4.0-beta.1",
    "react": "15.3.2",
    "react-dom": "15.3.2",
    "react-redux": "4.0.0",
    "redux": "3.0.4",
    "redux-devtools": "3.3.1",
    "redux-thunk": "2.1.0",
    "superagent": "2.3.0"
  },
  "devDependencies": {
    "babel-core": "6.3.13",
    "babel-loader": "6.2.0",
    "babel-polyfill": "6.3.14",
    "babel-preset-es2015": "6.3.13",
    "babel-preset-react": "6.3.13",
    "babel-preset-stage-0": "6.3.13",
    "copyfiles": "1.0.0",
    "css-loader": "0.25.0",
    "esdoc": "^0.4.8",
    "eslint": "3.7.1",
    "eslint-plugin-react": "6.4.1",
    "file-loader": "0.9.0",
    "glob": "7.0.3",
    "html-loader": "0.4.4",
    "html-webpack-plugin": "^2.24.1",
    "json-loader": "0.5.4",
    "nock": "9.0.2",
    "node-sass": "3.10.0",
    "nyc": "8.4.0",
    "pug": "2.0.0-beta6",
    "pug-loader": "2.3.0",
    "sass-loader": "4.0.2",
    "style-loader": "0.13.0",
    "stylus": "^0.54.5",
    "stylus-loader": "2.3.1",
    "tap-spec": "4.1.1",
    "tape": "4.5.1",
    "url-loader": "^0.5.7",
    "webpack": "2.1.0-beta.25",
    "webpack-dev-server": "2.1.0-beta.10"
  },
  "config": {
    "nyc": {
      "exclude": [
        "test.js",
        "src/**/_test_",
        "config"
      ]
    }
  }
}
